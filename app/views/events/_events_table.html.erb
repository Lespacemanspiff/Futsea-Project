<table>
  <thead>
    <tr>
      <th><%= "Location"%></th>
      <th><%= "City" %></th>
      <th><%= "Time" %></th>
      <th><%= "Price" %></th>
    </tr>
  </thead>
  <tbody>
    <% events.each do |event| %>
      <tr>
        <td><%= event.field.location.name %></td>
        <td><%= event.field.location.city %></td>
        <td><%= event.time %></td>
        <td><%= event.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>
